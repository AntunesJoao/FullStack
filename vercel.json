{
    "version": 2,
    "builds": [
      {
        "src": "backend/api.js",
        "use": "@vercel/node"
      },
      
      {
        "src": "project_glpi/App.jsx",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "backend/api.js"
      },
      {
        "src": "/(.*)",
        "dest": "frontend/$1"
      }
    ]
  }
  